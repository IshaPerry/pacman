<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pacman.ui.gameOverControls">

    <children>
        <ButtonBar layoutX="217.0" layoutY="319.0" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button id="playAgain" alignment="CENTER" mnemonicParsing="false" onAction="#hitPlayAgain" prefHeight="17.0" prefWidth="89.0" text="Play Again" />
                <Button id="exit" mnemonicParsing="false" onAction="#hitExit" text="Exit" />
            </buttons>
        </ButtonBar>
        <ImageView fitHeight="314.0" fitWidth="666.0" layoutX="-32.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@gameOver.png" />
            </image>
        </ImageView>
        <Label fx:id="gameOverLabel" layoutX="271.0" layoutY="229.0" prefHeight="17.0" prefWidth="111.0" textFill="#f8f4f4">
            <font>
                <Font name="Ayuthaya" size="20.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
